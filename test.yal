add_generic := .(T: type, a: T, b: T) -> T {
    return a + b;
};

main := .() {
    a := add_generic(i32, 10, 12);
    printf("r=%d\n", a);

    b := add_generic(i8, 10 as i8, 12 as i8);
    printf("r=%d\n", b as i32);
};

// TODO: Implement variadic functions
extern "printf" printf: .(fmt: [*]i8, arg0: i32) -> void;
