module main;

def Args = struct {
    argc: i32,
    argv: [*][*]u8,
};

func print_args(args: *Args) {
    var i = 0;
    while i < args.argc {
        c_printf("- args[%d]: %s\n".ptr, i, args.argv[i]);

        i = i + 1;
    }

    return;
}

func main(argc: i32, argv: [*][*]u8) i32 {
    var args: Args = .{ .argc=argc, .argv=argv };

    print_args(&args);
    return 0;
}

