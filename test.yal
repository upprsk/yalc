module main;

func main() i32 {
    var s = "this is a test string!";

    var spaces: usize = 0;
    var i: usize = 0;
    while i < s.len {
        defer i = i + 1;

        if s[i] == 0x20 {
            spaces = spaces + 1;
        }
    }

    c_printf("found %d spaces\n".ptr, spaces);
    return 0;
}

