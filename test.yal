extern "__builtin_print_i32"
    printi: .(n: i32) -> void;

extern "__builtin_print_char"
    printc: .(c: i32) -> void;

extern "__builtin_read_i32"
    readi: .() -> i32;

main := .() {
    a := 50;
    b := 25;

    printi(a);
    printc(32);
    printi(b);
    printc(10);

    printi(a + b);
    printc(10);

    printi(a - b);
    printc(10);

    printi(a * b);
    printc(10);

    printi(a / b);
    printc(10);
};
