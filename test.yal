int := i32;

export "main" main := .() -> int {
    c_printf("enter number:\n");

    v := 0;
    _ = c_scanf("%d", v&);

    while v > 0 {
        c_printf("[%d] hello!\n", v);
        v = v - 1;
    }

    return 0;
};

extern "printf" c_printf: .(fmt: [*:0]i8, ...) -> void;
extern "scanf" c_scanf: .(fmt: [*:0]i8, ...) -> i32;
